{
	"manifest_version": 2,
             "applications": {
                "gecko": {
                   "id": "TaskviewGridLayout@Sungho.Hwang",
                   "strict_min_version": "78.0"
                    }
             },
	"name": "TaskviewGridLayout",
	"version": "3.7",
	"description": "Taskview grid layout",
             "icons": {
               "32": "tvg32.png"
                 },
"browser_action": {
    "default_icon": "tvg32.png",
    "default_title": "TaskviewGridLayout",
    "default_popup": "popup.html"
  },
"permissions": [ "theme"
        ],
"theme_experiment": {
    "stylesheet": "style.css",
        "colors": {
  "box_bg": "--box-bg",
  "box_text": "--box-text"
        },
        "properties": {
  "display": "--display1",
  "width1": "--width1",
  "height1": "--height1",
  "grid_template_areas1": "--grid-template-areas1",
  "grid_template_columns1": "--grid-template-columns1",
  "grid_template_rows1": "--grid-template-rows1",
  "visibility2": "--visibility2",
  "width3": "--width3",
  "height3": "--height3",
  "grid_row3": "--grid-row3",
  "border3": "--border3",
  "border4": "--border4",
  "width4": "--width4",
  "height4": "--height4",
  "grid_row4": "--grid-row4"
    }
  }
}